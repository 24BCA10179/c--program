#include <stdio.h>

int main() {
    int number, i;
    printf("enter an integer to display its multiplication table: ");
    scanf("%d", &number);
    
    printf("multiplication table for %d; \n", number);
    for (i = 1; i <= 10; i++){
        printf("%d x %d = %d\n", number,i,number * i);
    }
    


    return 0;
}
